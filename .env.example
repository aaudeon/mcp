# Configuration du serveur MCP Weather
# API Open-Meteo utilisée (gratuite, sans clé requise)

# Port du serveur (optionnel - MCP utilise principalement stdio)
MCP_SERVER_PORT=3000

# Langue par défaut pour les réponses météo
DEFAULT_LANGUAGE=fr

# Environnement d'exécution
NODE_ENV=production

# Configuration optionnelle
# TIMEOUT_MS=5000
# MAX_FORECAST_DAYS=7
