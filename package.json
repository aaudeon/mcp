{
  "name": "mcp-weather-server",
  "version": "1.0.0",
  "description": "MCP server providing real-time weather information",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "demo": "npm run build && tsx src/demo.ts",
    "docker:build": "docker build -t mcp-weather-server .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mcp-weather-server"
  },
  "keywords": ["mcp", "weather", "llm", "model-context-protocol"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
